{
  "private": true,
  "packageManager": "pnpm@7.25.0",
  "scripts": {
    "dev": "vite --open --host",
    "build": "vite build",
    "preview": "vite preview --port 5050 --host",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type:vuetify": "node ./scripts/vuetify-type.mjs > ./src/vuetify2.d.ts"
  },
  "dependencies": {
    "@mdi/js": "^7.1.96",
    "@vueuse/core": "^9.11.0",
    "axios": "^1.2.3",
    "echarts": "^5.4.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.29",
    "portal-vue": "^2.1.7",
    "screenfull": "^6.0.2",
    "vue": "^2.7.14",
    "vue-echarts": "^6.5.1",
    "vue-i18n": "8.27.2",
    "vue-i18n-bridge": "^9.2.2",
    "vue-router": "^3.6.5",
    "vuetify": "^2.6.14"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.8.1",
    "@kingyue/vite-plugin-vue2-svg": "^0.5.0",
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/vue": "^5.8.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.18.11",
    "@types/nprogress": "^0.2.0",
    "@types/postcss-preset-env": "^7.7.0",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^1.3.3",
    "browserslist-to-esbuild": "^1.2.0",
    "cypress": "^10.11.0",
    "eslint": "^8.32.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "flush-promises": "^1.0.2",
    "jsdom": "^21.0.0",
    "msw": "^0.49.2",
    "postcss-load-config": "^4.0.1",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.3",
    "sass": "~1.32.13",
    "start-server-and-test": "^1.15.2",
    "terser": "^5.16.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.4",
    "vite-plugin-browserslist-useragent": "^0.2.2",
    "vite-plugin-inspect": "^0.7.14",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vitest": "^0.27.2",
    "volar-plugin-vue-router": "^0.0.2",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.0.24"
  },
  "browserslist": [
    "> 1.3%",
    "last 2 versions",
    "not dead",
    "not op_mini all",
    "not ie>0"
  ],
  "msw": {
    "workerDirectory": "public"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vite": "4"
      }
    }
  }
}
